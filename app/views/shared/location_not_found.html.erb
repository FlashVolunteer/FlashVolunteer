<div class="row">
	<div class="col-md-6">
		<%= image_tag('graphic_404.png') %>
	</div>
	<div class="col-md-6">
		<h1>Hmm... I can't find that city!</h1>
		<p>You should go find more volunteer opportunities in one of the cities below:</p>
		<ul>
          <% Hub.all.sort_by{|hub| hub.city_state}.each do |hub| %>
             <li><%= link_to hub.city_state, hub_url(hub.name) %></li>
          <% end %>
        </ul>
	</div>
</div>
