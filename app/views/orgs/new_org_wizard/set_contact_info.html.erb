<% content_for :question do %>
    Is this info about your group correct?
<% end %>
<% content_for :below_question do %>
    <div class="row-fluid">
        <div class="span2">
            <label>name</label>
        </div>
        <div class="span10">
            <%= text_field(:org, :name) %>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span2">
            <label>email</label>
        </div>
        <div class="span10">
            <%= text_field(:org, :email) %>
        </div>
    </div>
<% end %>
<% content_for :button do %>
    <%= submit_tag("ask me more", :class => "btn btn-primary pull-right") %>
<% end %>
<%= content_for :form_template do 'orgs/new_org_wizard/question_with_form' end %>
<%= render(:template => 'shared/splash_with_question') %>