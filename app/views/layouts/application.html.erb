<!DOCTYPE html>
<html>
<head>
  <title>Flashvolunteer</title>
  <%= stylesheet_link_tag :all %>
  <%= theme_stylesheet_link_tag 'colors' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="container">
	<%= render"shared/header" %>
	<div id="content">
		<div id="bottom">
			<div id="columns">
				<%= yield :title %>
			</div>
			<%= yield %>
			<div class="clear"> </div>
		</div>
	</div>
	<div id="clearfooter"></div>
</div>
<%= render"shared/footer" %>
</body>
</html>
